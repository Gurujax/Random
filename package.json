    "browser-sync": "^2.14.0",
    "chai": "^3.5.0",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1"
  },
  "engines": {
    "node": ">=4.6",
    "npm": ">=2.15"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sans712/HTMLPortfolioProject.git"
  },
  "bugs": {
    "url": "https://github.com/sans712/HTMLPortfolioProject/issues"
  },
